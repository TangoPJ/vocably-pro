{
  "manifest_version": 3,

  "name": "Vocably",
  "description": "Vocably Extension",
  "version": "0.0.1",

  "options_ui": {
    "page": "options.html"
  },

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "exclude_matches": ["http://localhost:8010/", "http://localhost:8020/"]
    }
  ],

  "permissions": ["storage"],

  "host_permissions": ["<all_urls>"]
}
