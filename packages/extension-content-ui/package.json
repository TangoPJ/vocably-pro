{
  "name": "@vocably/extension-content-ui",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/extension-content-ui/extension-content-ui.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "build-watch": "stencil build --watch",
    "start": "stencil build --config stencil.serve.ts --dev --watch --serve",
    "test": "echo 'Use `yarn integration` to test @vocably/extension-content-ui.'",
    "integration": "stencil test --spec --e2e",
    "integration.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "extension-content-ui": "npm-run-all --parallel start build-watch"
  },
  "dependencies": {
    "@vocably/model": "0.0.0",
    "@vocably/sulna": "0.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3"
  },
  "license": "MIT"
}
